<Window x:Class="RecordSocialServicesProvision.WindowCMD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RecordSocialServicesProvision"
        mc:Ignorable="d"
        
        Title="Командная строка" 
        Width="1920" Height="1080" 
        WindowState="Maximized" 
        FontFamily="{StaticResource BasicFont}" FontSize="16"
        Icon="/res/icon/MainIcon.png">

    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" TextWrapping="Wrap" AcceptsReturn="True" Margin="10" TextChanged="TextBox_TextChanged" PreviewKeyDown="TextBox_PreviewKeyDown"/>
    </Grid>

</Window>
